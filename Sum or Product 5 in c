#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int a,b,c,d;
    scanf("%d%d%d%d",&a,&b,&c,&d);
    c=a*b;
    d=a+b;
    if(c>1000)
    {
        printf("%d",d);
    }
        else
        {
            printf("%d",c);
        }
    return 0;
}
