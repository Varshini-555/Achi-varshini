import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        for(int i=1;i<=n;i++){
            for(int j=(i)*(i-1)/2+1;j<=(i)*(i+1)/2;j++){
                System.out.print(j+" ");
            }
            System.out.println();
    }
}
}
